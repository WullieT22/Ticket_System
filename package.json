{
  "name": "it-ticket-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "start-system": "powershell -ExecutionPolicy Bypass -File ./start-ticket-system.ps1",
    "start-browser": "powershell -ExecutionPolicy Bypass -File ./start-with-browser.ps1",
    "start-mac": "chmod +x ./start-ticket-system.sh && ./start-ticket-system.sh",
    "start-mac-browser": "chmod +x ./start-with-browser.sh && ./start-with-browser.sh",
    "setup": "npm install && npm audit fix",
    "dev-open": "npm run dev & sleep 3 && open http://localhost:3000",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "next": "^14.2.33",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.7"
  }
}
